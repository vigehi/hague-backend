<div id="<%= dom_id user_type %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Type name:</strong>
    <%= user_type.type_name %>
  </p>
  <% if action_name != "show" %>
    <%= link_to "Show this user type", user_type, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to "Edit this user type", edit_user_type_path(user_type), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
